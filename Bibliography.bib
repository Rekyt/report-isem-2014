	number = {4},
	urldate = {2013-12-02},
	journal = {The Plant Journal},
	author = {Wang, Yun and Fan, Guangyi and Liu, Yiman and Sun, Fengming and Shi, Chengcheng and Liu, Xin and Peng, Jing and Chen, Wenbin and Huang, Xinfang and Cheng, Shifeng and Liu, Yuping and Liang, Xinming and Zhu, Honglian and Bian, Chao and Zhong, Lan and Lv, Tian and Dong, Hongxia and Liu, Weiqing and Zhong, Xiao and Chen, Jing and Quan, Zhiwu and Wang, Zhihong and Tan, Benzhong and Lin, Chufa and Mu, Feng and Xu, Xun and Ding, Yi and Guo, An-Yuan and Wang, Jun and Ke, Weidong},
	year = {2013},
	keywords = {basal eudicots, sacred lotus (Nelumbo nucifera) genome, seed dormancy and development, starch synthesis, γ whole genome duplication},
	pages = {557--567},
	file = {Full Text PDF:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/3UUTKSR5/Wang et al. - 2013 - The sacred lotus genome provides insights into the.pdf:application/pdf;Snapshot:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/J8DVWK2U/abstract\;jsessionid=F5336FEC4BCDE0D5DB4C60F1D9CDBF5C.html:text/html}
}

@article{shlyueva_transcriptional_2014,
