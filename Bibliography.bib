@article{marshall_alternative_2013,
	title = {Alternative Splicing and Subfunctionalization Generates Functional Diversity in Fungal Proteomes},
	volume = {9},
	url = {http://dx.doi.org/10.1371/journal.pgen.1003376},
	doi = {10.1371/journal.pgen.1003376},
	abstract = {Author {SummaryThe} role of duplicated genes in originating new functions is an important question in evolution. Almost all species have duplicated genes that carry out similar but not identical functions. Similar proteins that perform different functions can also be generated when one gene generates multiple {mRNAs} by alternative splicing that are translated into multiple similar proteins. This alternative splicing is prevalent in animal cells, but much rarer in fungi. Here we show that most fungi use alternative splicing to make a Ski7 protein and a Hbs1 protein from the same gene. Two fungi, budding yeast and fission yeast, have been much better characterized than other fungi, and co-incidentally they both have duplicated this alternatively spliced gene, resulting in two similar genes that are no longer alternatively spliced. Finally, we describe another example where two duplicate genes replace one alternatively spliced gene, suggesting that this is a common mechanism to divide functions among duplicate genes.},
	number = {3},
	urldate = {2013-12-02},
	journal = {{PLoS} Genet},
	author = {Marshall, Alexandra N. and Montealegre, Maria Camila and Jiménez-López, Claudia and Lorenz, Michael C. and van Hoof, Ambro},
	month = mar,
	year = {2013},
	pages = {e1003376},
	file = {PLoS Full Text PDF:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/RDERE4P4/Marshall et al. - 2013 - Alternative Splicing and Subfunctionalization Gene.pdf:application/pdf;PLoS Snapshot:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/8CXS95JN/infodoi10.1371journal.pgen.html:text/html}
}

@article{force_preservation_1999,
