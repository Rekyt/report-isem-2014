	month = feb,
	year = {2013},
	keywords = {{CARRIER} proteins, {COMPARATIVE} studies, {DNA}-binding proteins, {DNA} microarrays, important, {NUCLEOTIDE} sequence, {TRANSCRIPTION} factors},
