	pages = {1257--1257},
	file = {Full Text PDF:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/F5TTF767/Mayrose et al. - 2011 - Recently Formed Polyploid Plants Diversify at Lowe.pdf:application/pdf;Snapshot:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/EZRBM6QJ/1257.html:text/html}
