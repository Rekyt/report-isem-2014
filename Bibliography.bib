	file = {Full Text PDF:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/C37F5SWB/Mable et al. - 2011 - Genome duplication in amphibians and fish an exte.pdf:application/pdf;Snapshot:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/6Q92GIUN/full.html:text/html}
