@article{bernard_explain_2013,
	title = {Explain Bioinformatics to Your Grandmother!},
	volume = {9},
	url = {http://dx.doi.org/10.1371/journal.pcbi.1003305},
	doi = {10.1371/journal.pcbi.1003305},
	abstract = {What are you working on? You have certainly been asked that question many times, whether it be at a Saturday night party, during a discussion with your neighbors, or at a family gathering. Communicating with a lay audience about scientific subjects and making them attractive is a difficult task. But difficult or not, you will have to do it for many years, not only with your family and friends, but also with your colleagues and collaborators. So, better learn now! Although not usually taught, the ability to explain your work to others is an essential skill in science, where communication plays a key role. Using some examples of the French Regional Student Group activities, we discuss here (i) why it is important to have such communication skills, (ii) how you can get involved in these activities by using existing resources or working with people who have previous experience, and (iii) what you get out of this amazing experience. We aim to motivate you and provide you with tips and ideas to get involved in promoting scientific activities while getting all the benefits.},
	number = {10},
	urldate = {2013-11-01},
	journal = {{PLoS} Comput Biol},
	author = {Bernard, Virginie and Michaut, Magali},
