	author = {Lin, Zhenguo and Wang, Tzi-Yuan and Tsai, Bing-Shi and Wu, Fang-Ting and Yu, Fu-Jung and Tseng, Yu-Jung and Sung, Huang-Mo and Li, Wen-Hsiung},
	month = jan,
	year = {2013},
	pmid = {23650209},
	keywords = {{CBF}1, cis-regulation, Evolution, fermentation, Gene Expression, yeast},
	pages = {1065--1078},
	file = {Full Text PDF:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/FZE9QNBF/Lin et al. - 2013 - Identifying Cis-Regulatory Changes Involved in the.pdf:application/pdf;Snapshot:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/RW85JE6P/1065.html:text/html}
