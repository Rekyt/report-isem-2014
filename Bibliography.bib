@article{schwaiger_evolutionary_2014,
	title = {Evolutionary conservation of the eumetazoan gene regulatory landscape},
	volume = {24},
	issn = {1088-9051, 1549-5469},
	url = {http://genome.cshlp.org/content/24/4/639},
	doi = {10.1101/gr.162529.113},
	abstract = {Despite considerable differences in morphology and complexity of body plans among animals, a great part of the gene set is shared among Bilateria and their basally branching sister group, the Cnidaria. This suggests that the common ancestor of eumetazoans already had a highly complex gene repertoire. At present it is therefore unclear how morphological diversification is encoded in the genome. Here we address the possibility that differences in gene regulation could contribute to the large morphological divergence between cnidarians and bilaterians. To this end, we generated the first genome-wide map of gene regulatory elements in a nonbilaterian animal, the sea anemone Nematostella vectensis. Using chromatin immunoprecipitation followed by deep sequencing of five chromatin modifications and a transcriptional cofactor, we identified over 5000 enhancers in the Nematostella genome and could validate 75\% of the tested enhancers in vivo. We found that in Nematostella, but not in yeast, enhancers are characterized by the same combination of histone modifications as in bilaterians, and these enhancers preferentially target developmental regulatory genes. Surprisingly, the distribution and abundance of gene regulatory elements relative to these genes are shared between Nematostella and bilaterian model organisms. Our results suggest that complex gene regulation originated at least 600 million yr ago, predating the common ancestor of eumetazoans.},
	language = {en},
	number = {4},
	urldate = {2014-04-09},
	journal = {Genome Res.},
	author = {Schwaiger, Michaela and Schönauer, Anna and Rendeiro, André F. and Pribitzer, Carina and Schauer, Alexandra and Gilles, Anna F. and Schinko, Johannes B. and Renfer, Eduard and Fredman, David and Technau, Ulrich},
	month = jan,
	year = {2014},
	pmid = {24642862},
	pages = {639--650},
	file = {Full Text PDF:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/2CSK22E8/Schwaiger et al. - 2014 - Evolutionary conservation of the eumetazoan gene r.pdf:application/pdf;Snapshot:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/T2XPAZPN/639.html:text/html}
}

