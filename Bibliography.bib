@article{asrar_fourfold_2013,
	title = {Fourfold paralogy regions on human {HOX}-bearing chromosomes: Role of ancient segmental duplications in the evolution of vertebrate genome},
	volume = {66},
	issn = {1055-7903},
	shorttitle = {Fourfold paralogy regions on human {HOX}-bearing chromosomes},
	url = {http://www.sciencedirect.com/science/article/pii/S1055790312004307},
	doi = {10.1016/j.ympev.2012.10.024},
	abstract = {Background
Susumu Ohno’s idea that modern vertebrates are degenerate polyploids (concept referred as 2R hypothesis) has been the subject of intense debate for past four decades. It was proposed that intra-genomic synteny regions (paralogons) in human genome are remains of ancient polyploidization events that occurred early in the vertebrate history. The quadruplicated paralogon centered on human {HOX} clusters is taken as evidence that human {HOX}-bearing chromosomes were structured by two rounds of whole genome duplication ({WGD}) events.
Results
Evolutionary history of human {HOX}-bearing chromosomes (chromosomes 2/7/12/17) was evaluated by the phylogenetic analysis of multigene families with triplicated or quadruplicated distribution on these chromosomes. Topology comparison approach categorized the members of 44 families into four distinct co-duplicated groups. Distinct gene families belonging to a particular co-duplicated group, exhibit similar evolutionary history and hence have duplicated simultaneously, whereas genes of two distinct co-duplicated groups do not share their evolutionary history and have not duplicated in concert with each other.
Conclusion
The recovery of co-duplicated groups suggests that “ancient segmental duplications and rearrangements” is the most rational model of evolutionary events that have generated the triplicated and quadruplicated paralogy regions seen on the human {HOX}-bearing chromosomes.},
	number = {3},
	urldate = {2014-01-11},
	journal = {Molecular Phylogenetics and Evolution},
	author = {Asrar, Zainab and Haq, Farhan and Abbasi, Amir Ali},
	month = mar,
	year = {2013},
	keywords = {{HOX} cluster, Intra-genomic synteny, Multigene Family, Phylogeny, Segmental duplication},
	pages = {737--747},
	file = {ScienceDirect Full Text PDF:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/U4R77HGT/Asrar et al. - 2013 - Fourfold paralogy regions on human HOX-bearing chr.pdf:application/pdf;ScienceDirect Snapshot:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/AM89366Z/S1055790312004307.html:text/html}
}

@article{canestro_transposon_2012,
	title = {Transposon diversity is higher in amphioxus than in vertebrates: functional and evolutionary inferences},
	volume = {11},
	issn = {2041-2649, 2041-2657},
	shorttitle = {Transposon diversity is higher in amphioxus than in vertebrates},
	url = {http://bfg.oxfordjournals.org.docelec.univ-lyon1.fr/content/11/2/131},
	doi = {10.1093/bfgp/els010},
	abstract = {Transposable elements ({TEs}) are main components of eukaryote genomes—up to 50\% in some vertebrates—which can replicate and jump to new locations. {TEs} contribute to shape genome evolution, actively by creating new genes (or exons) or altering gene expression as consequence of transposition, and passively by serving as illegitimate recombinational hotspots. Analysis of amphioxus {TEs} can help to shed light on the ancestral status of chordate {TEs} and to understand genome evolution in cephalochordates and early vertebrates. The Branchiostoma floridae genome project has revealed that {TE} content constitutes ∼28\% of the amphioxus genome. Amphioxus {TEs} belong to more than 30 superfamilies, which represent a higher diversity than in vertebrates. Amphioxus {TE} families are also highly heterogeneous as generally none of their members are drastically more abundant than others, and none of the {TEs} seems to have suffered any massive expansion. Such diversity and heterogeneity make the amphioxus genome not to be particularly prone to major evolutionary changes mediated by {TEs}, and therefore favoring genomic evolutionary stasis. Comparison of {TE} diversity and content between amphioxus and vertebrates allows us to discuss whether or not a burst of {TEs} happened after the two rounds of whole-genome duplication that occurred during early vertebrate evolution.},
	language = {en},
	number = {2},
	urldate = {2013-12-02},
	journal = {Briefings in Functional Genomics},
	author = {Cañestro, Cristian and Albalat, Ricard},
	month = jan,
	year = {2012},
	pmid = {22389043},
	keywords = {Amphioxus cephalochordates, Chordate and vertebrate evolution, {DNA} methylation, Genome stasis, Transposable elements, Two rounds of whole-genome duplication},
	pages = {131--141},
	file = {00037f6936429870c3058d254f4f868c.pdf:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/62BZ93FT/00037f6936429870c3058d254f4f868c.pdf:application/pdf;Snapshot:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/FWPIDDCZ/131.html:text/html}
