	pages = {e1002458},
	file = {PLoS Full Text PDF:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/QIXZJAE8/Blair et al. - 2012 - What Can Causal Networks Tell Us about Metabolic P.pdf:application/pdf;PLoS Snapshot:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/ZQCDARJT/infodoi10.1371journal.pcbi.html:text/html}
}

@article{guidetti_dormancy_2011,
	title = {On dormancy strategies in tardigrades},
	volume = {57},
	issn = {0022-1910},
	shorttitle = {Dormancy and Developmental Arrest in Invertebrates},
	url = {http://www.sciencedirect.com/science/article/pii/S0022191011000692},
	doi = {10.1016/j.jinsphys.2011.03.003},
	abstract = {In this review we analyze the dormancy strategies of metazoans inhabiting “hostile to life” habitats, which have a strong impact on their ecology and in particular on the traits of their life history. Tardigrades are here considered a model animal, being aquatic organisms colonizing terrestrial habitats. Tardigrades evolved a large variety of dormant stages that can be ascribed to diapause (encystment, cyclomorphosis, resting eggs) and cryptobiosis (anhydrobiosis, cryobiosis, anoxibiosis). In tardigrades, diapause and cryptobiosis can occur separately or simultaneously, consequently the adoption of one adaptive strategy is not necessarily an alternative to the adoption of the other. Encystment and cyclomorphosis are characterized by seasonal cyclic changes in morphology and physiology of the animals. They share several common features and their evolution is strictly linked to the molting process. A bet-hedging strategy with different patterns of egg hatching time has been observed in a tardigrade species. Four categories of eggs have been identified: subitaneous, delayed-hatching, abortive and diapause resting eggs, which needs a stimulus to hatch (rehydration after a period of desiccation). Cryptobiotic tardigrades are able to withstand desiccation (anhydrobiosis) and freezing (cryobiosis) at any stage of their life-cycle. This ability involves a complex array of factors working at molecular (bioprotectans), physiological and structural levels. Animal survival and the accumulation of molecular damage are related to the time spent in the cryptobiotic state, to the abiotic parameters during the cryptobiotic state, and to the conditions during initial and final phases of the process. Cryptobiosis evolved independently at least two times in tardigrades, in eutardigrades and in echiniscoids. Within each evolutionary line, the absence of cryptobiotic abilities is more related to selective pressures to local habitat adaptation than to phylogenetic relationships. The selective advantages of cryptobiosis (e.g. persistency in “hostile to life” habitats, reduction of competitors, parasites and predators, escaping in time from stressful conditions) could explain the high tardigrade species diversity and number of specimens found in habitats that dry out compared to freshwater habitats.},
	number = {5},
	urldate = {2012-12-31},
	journal = {Journal of Insect Physiology},
	author = {Guidetti, Roberto and Altiero, Tiziana and Rebecchi, Lorena},
	month = may,
	year = {2011},
	keywords = {Anhydrobiosis, Cryobiosis, Cyclomorphosis, Encystment, Evolution, Resting eggs, Tardigrades},
	pages = {567--576},
	file = {ScienceDirect Full Text PDF:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/EBNKXWCH/Guidetti et al. - 2011 - On dormancy strategies in tardigrades.pdf:application/pdf;ScienceDirect Snapshot:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/AZFU45C4/S0022191011000692.html:text/html}
}

@article{lim_design_2013,
	title = {Design Principles of Regulatory Networks: Searching for the Molecular Algorithms of the Cell},
	volume = {49},
	issn = {1097-2765},
