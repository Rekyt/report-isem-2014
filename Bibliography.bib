@article{sperling_remembrance_2011,
	series = {The genome organisation of eukaryotic microbes},
	title = {Remembrance of things past retrieved from the Paramecium genome},
	volume = {162},
	issn = {0923-2508},
	url = {http://www.sciencedirect.com/science/article/pii/S0923250811000386},
	doi = {10.1016/j.resmic.2011.02.012},
	abstract = {Paramecium and other ciliates are the only unicellular eukaryotes that separate germinal and somatic functions. A germline micronucleus transmits the genetic information to sexual progeny, while a somatic macronucleus expresses the genetic information during vegetative growth to determine the phenotype. At each sexual generation, a new macronucleus develops from the zygotic nucleus through programmed rearrangements of the germline genome. Paramecium tetraurelia somatic genome sequencing, reviewed here, has provided insight into the organization and evolution of the genome. A series of at least 3 whole genome duplications was detected in the Paramecium lineage and selective pressures that determine the fate of the gene duplicates analyzed. Variability in the somatic {DNA} was characterized and could be attributed to the genome rearrangement processes. Since, in Paramecium, alternative genome rearrangement patterns can be inherited across sexual generations by homology-dependent epigenetic mechanisms and can affect phenotype, I discuss the possibility that ciliate nuclear dimorphism buffers genetic variation hidden in the germline.},
	number = {6},
	urldate = {2014-05-13},
	journal = {Research in Microbiology},
	author = {Sperling, Linda},
	month = jul,
	year = {2011},
	keywords = {epigenetics, Evolution, Genome rearrangements, moyen, read, review, Small-{RNA}, speciation, whole genome duplication},
	pages = {587--597},
	file = {ScienceDirect Full Text PDF:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/TRS9N7C8/Sperling - 2011 - Remembrance of things past retrieved from the Para.pdf:application/pdf;ScienceDirect Snapshot:/Users/Rekyt/Library/Application Support/Firefox/Profiles/vkw1imse.default-1412076110318/zotero/storage/8TE53NN3/S0923250811000386.html:text/html}
